{
  "name": "xcm-mono",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:demo": "cd ./apps/demo && yarn dev",
    "dev:hook": "tsc -p ./packages/react-hook --watch",
    "dev:lib": "tsc -p ./packages/lib --watch",
    "dev:types": "tsc -p ./packages/types --watch",
    "dev": "concurrently \"yarn dev:types\" \"yarn dev:hook\" \"yarn dev:lib\" \"yarn dev:squid\" \"yarn dev:squid:serve\" \"yarn dev:demo\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "concurrently": "^7.3.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.10"
}
